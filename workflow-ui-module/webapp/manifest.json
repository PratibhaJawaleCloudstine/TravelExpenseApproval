{
    "_version": "1.26.0",
    "sap.app": {
        "id": "travelexpensensp.workflowuimodule",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.1"
        },
        "title": "{{ appTitle }}",
        "description": "{{ appDescription }}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.4",
            "toolsId": "01a1a68f-0115-450d-bdf3-3a485408a319"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "travelexpensensp.workflowuimodule.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "travelexpensensp.workflowuimodule.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteTravelExpenseApprovalForm",
                    "pattern": ":?query:",
                    "target": ["TargetTravelExpenseApprovalForm"]
                }
            ],
            "targets": {
                "TargetTravelExpenseApprovalForm": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "TravelExpenseApprovalForm",
                    "viewName": "TravelExpenseApprovalForm"
                }
            }
        },
        "rootView": {
            "viewName": "travelexpensensp.workflowuimodule.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "travelexpensesapproval"
    },
    "sap.bpa.task": {
        "_version": "1.0.0",
        "outcomes": [
            {
                "id": "approve",
                "label": "Approve"
            }, {
                "id": "reject",
                "label": "Reject"
            }
        ],
        "inputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "input",
            "type": "object",
            "required": [
                
            ],
            "properties": {
                "travelDetails": {
                    "type": "object",
                    "title": "Travel Request Details",
                    "required": [],
                    "properties": {
                        "travelDetailsId": {
                            "type": "string",
                            "title": "Travel Request ID",
                            "description": "Travel Request ID"
                        },
                        "TravelStartDate": {
                            "type": "string",
                            "title": "Travel Start Date",
                            "description": "Travel Start Date"
                        },
                        "TravelEndDate": {
                            "type": "string",
                            "title": "Travel End Date",
                            "description": "Travel End Date"
                        },
                        "TravelDeparture": {
                            "type": "string",
                            "title": "Travel Departure",
                            "description": "Travel Departure"
                        },
                        "TravelArrival": {
                            "type": "string",
                            "title": "Travel Arrival",
                            "description": "Travel Arrival"
                        },
                        "TravelPlaceOfVisit": {
                            "type": "string",
                            "title": "Travel Place Of Visit",
                            "description": "Travel Place Of Visit"
                        },
                        "TravelEstimationCost": {
                            "type": "string",
                            "title": "Travel Estimation Cost",
                            "description": "Travel Estimation Cost"
                        },
                        "TravelSelfTravel": {
                            "type": "string",
                            "title": "Self Travel",
                            "description": "Self Travel"
                        },
                        "TravelPurposeOfTravel": {
                            "type": "string",
                            "title": "Purpose of Travel",
                            "description": "Purpose of Travel"
                        },
                        "TravelEmployeeId": {
                            "type": "string",
                            "title": "Travel Employee Id",
                            "description": "Travel Employee Id"
                        },
                        "TravelEmployeeEmail": {
                            "type": "string",
                            "title": "Travel Employee Email",
                            "description": "Travel Employee Email"
                        },
                        "TravelEmployeeName": {
                            "type": "string",
                            "title": "Travel Employee Name",
                            "description": "Travel Employee Name"
                        }
                    }
                },
                "travelExpenses": {
                    "type": "array",
                    "title": "travel Expenses",
                    "required": [],
                    "items": {
                        "type": "object",
                        "properties": {
                            "expenseType": {
                                "type": "string",
                                "title": "Expense Type"
                            },
                            "receiptAmount": {
                                "type": "string",
                                "title": "Receipt Amount"
                            },
                            "receiptDate": {
                                "type": "string",
                                "title": "Receipt Date"
                            }
                        }
                    }
                }
            }
        },
        "outputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "output",
            "type": "object",
            "required": [],
            "properties": {
                "comment": {
                    "type": "string",
                    "title": "Comment",
                    "description": "Comment to buyer"
                }
            }
        },
        "category": "approval"
    }
}